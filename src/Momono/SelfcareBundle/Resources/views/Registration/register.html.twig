{% extends "FOSUserBundle:Registration:register.html.twig" %}
 
{% block fos_user_login_form %}
<form action="{{ path('selfcare_user_registration') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_errors(form) }}
 
    {{ form_widget(form) }}
    <div class="mt3">
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="mbtn mbtn-large mbtn-shadow mbtn-salmon" />
    </div>
</form>
{% endblock %}